// prisma/schema.prisma
datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Destination {
  id          Int     @id @default(autoincrement())
  name        String
  location    String
  climate     String
  description String
}
model Kontak {
  id       Int      @id @default(autoincrement())
  nama     String
  email    String
  pesan    String
  tanggal  DateTime @default(now())
}
